                <div class="custom-file">
                    <input type="file" asp-for="FormFile" class="custom-file-input" id="inputGroupFile02">
                    <label class="custom-file-label" asp-for="FormFile" aria-describedby="inputGroupFileAddon02">Choose file</label>
                </div>
            </div>

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
